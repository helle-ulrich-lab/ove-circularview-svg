{
  "name": "node-http-server",
  "version": "1.0.0",
  "description": "Node.js example server starter project",
  "scripts": {
    "build:client": "esbuild src/index.js --bundle --outfile=dist/bundle.js --loader:.js=jsx --platform=node",
    "build:server": "esbuild server/server.jsx --bundle --outfile=build/server.js --platform=node --loader:.js=jsx",
    "build": "npm run build:client && npm run build:server",
    "start": "node ./build/server.js"
  },
  "dependencies": {
    "@teselagen/bio-parsers": "0.4.1",
    "ve-range-utils": "^2.6.6",
    "ve-sequence-utils": "^5.2.9",
    "classnames": "^2.3.2",
    "paths-js": "^0.4.11",
    "node-interval-tree": "^2.1.2",
    "esbuild": "0.19.5",
    "express": "4.18.2",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "nodemon": "1.18.4"
  },
  "keywords": []
}